@model IEnumerable<DTO.DTOEntity.OrderCardDTO>
@{
    ViewData["Title"] = "Home Page";
}


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox ">
                <div class="ibox-content">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover dataTables-example">
                            <thead>
                                <tr>
                                    <th>Ad,Soyad,Ata adı</th>
                                    <th>Card №</th>
                                    <th>Finkod</th>
                                    <th>Şirkət</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr class="gradeX">
                                        <td>@item.Name @item.Surname @item.FatherName</td>
                                        <td>@item.CarNo</td>
                                        <td>@item.Fincode</td>
                                        <td>@item.Company</td>
                                        <td>@item.Status</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

